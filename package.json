    "@stina/extension-api": "^0.24.0",
    "@types/archiver": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "archiver": "^7.0.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
